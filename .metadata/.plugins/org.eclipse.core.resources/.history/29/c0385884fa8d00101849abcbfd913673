package consoletest;

public class ConsoleTest {
	public static void main(String[] args) {
		
//     한 줄 주석(ctrl + /)
//		1. 설명 글을 작성할 때
//		2. 지금 당장 사용하지 않는 코드를 번역하고 싶지 않을 때ㅐ
		
// 		범위 주석 " /**/ " (ctrl + shift + /)
// 		1. 코드 사이에 주석을 처리하고 싶을 때
		System.out.println("hello, world!");
		System.out.println("김세환 병신 새끼!");
		System.out.print("전민철 존잘");
		
		
	}
}
